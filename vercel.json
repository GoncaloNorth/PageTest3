{
    "version": 2,
    "builds": [
        {
            "src": "**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/snake",
            "dest": "/snake/index.html"
        },
        {
            "src": "/pong",
            "dest": "/pong/index.html"
        },
        {
            "src": "/ballers",
            "dest": "/ballers/index.html"
        },
        {
            "src": "/bouncing",
            "dest": "/bouncing/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
} 
